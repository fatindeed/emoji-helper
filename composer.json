{
    "name": "fatindeed/emoji-helper",
    "description": "Emoji helper is a tool for you to escape emoji characters.",
    "license": "MIT",
    "homepage": "https://github.com/fatindeed/emoji-helper",
    "keywords": ["emoji", "laravel", "lumen", "middleware"],
    "authors": [
        {
            "name": "James Zhu",
            "homepage": "https://github.com/fatindeed"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0 || ^6.0 || ^7.0 || ^8.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Fatindeed\\EmojiHelper\\": "src"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "scripts": {
        "test": [
            "phpcs",
            "phpunit"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
